    "/store/nearby": {
      "get": {
        "description": "Returns stores within a specified radius of the given coordinates, sorted by distance. Uses the Haversine formula to calculate distances in kilometers.",
        "operationId": "StoreController_findNearby",
        "parameters": [
          {
            "name": "latitude",
            "required": true,
            "in": "query",
            "description": "Latitude of the search center point",
            "schema": {
              "example": 10.3157,
              "type": "number"
            }
          },
          {
            "name": "longitude",
            "required": true,
            "in": "query",
            "description": "Longitude of the search center point",
            "schema": {
              "example": 123.8854,
              "type": "number"
            }
          },
          {
            "name": "radius",
            "required": false,
            "in": "query",
            "description": "Search radius in kilometers (default: 10km, max: 50 results)",
            "schema": {
              "example": 5,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns array of stores with calculated distance field, ordered by proximity"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Find stores near a location",
        "tags": [
          "Stores"
        ]
      }
    },
